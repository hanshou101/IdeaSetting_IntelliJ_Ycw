      <option name="FOREGROUND" value="4b9649" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
</application>